{{ header }} {{ column_left }} {{ column_right }}
<div id="content" class="container">
	{{ content_top }}
	<ul class="breadcrumb">
		{% for breadcrumb in breadcrumbs %}
			<li><a href="{{ breadcrumb['href'] }}">{{ breadcrumb['text'] }}</a></li>
		{% endfor %}
	</ul>
	<h1>{{ heading_title }}</h1>
	<p>{{ description }}</p>
	{{ content_bottom }}
</div>
{{ footer }}